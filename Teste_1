int somaMultiplosDeTresOuCinco(int numero) {
  int soma = 0;

  for (int i = 1; i < numero; i++) {
    if (i % 3 == 0 || i % 5 == 0) {
      soma += i;
    }
  }

  return soma;
}

void main() {
  print(somaMultiplosDeTresOuCinco(10)); // Aqui é onde tem que adicionar os números desejados
  print(somaMultiplosDeTresOuCinco(11)); 
}
